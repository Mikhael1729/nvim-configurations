import React from "react"
import { makeStyles } from "@material-ui/core/styles"
import Table from "components/table";
import IHeadCell from "models/ihead-cell";

const productType: string[] = [
  "Cuenta de Ahorros",
  "Tarjeta de Credito Terminada en 5674",
  "Tarjeta de Debito Terminada en 3451",
]

interface IData {
  id: number,
  date: Date,
  description: string,
  productType: string,
  amount: string
}

function transTable({ id, date, description, productType, amount }: IData): IData {
  return { id, date, description, productType, amount }
}

const row = [
  transTable({
    id: 1,
    date: new Date(Date.now() + 1),
    description: "Cargo Edesur",
    productType: productType[0],
    amount: "22,345.67"
  }),
  transTable({
    id: 2,
    date: new Date(Date.now()),
    description: "Retiro Efectivo",
    productType: productType[2],
    amount: "1,345.07"
  }),
  transTable({
    id: 3,
    date: new Date(Date.now()),
    description: "Cargo Seguro BSP",
    productType: productType[1],
    amount: "22,345.67"
  }),
  transTable({
    id: 4,
    date: new Date(Date.now()),
    description: "Cargo CAASD",
    productType: productType[2],
    amount: "2,434.46"
  }),
]

function TransacTable() {
  const headerCells: IHeadCell<IData>[] = [
    { label: "Fecha", propertyName: "date", renderValue: (element: IData) => element.date.toString() },
    { label: "Descripción", propertyName: "description" },
    { label: "Descripción", propertyName: "description" },
  ]

    <TableCell>{row[propertyName]}</TableCell>

  return (
    <>
      <Table data={row} headerCells={headerCells} />
    </>
  )
}
export default TransacTable
