import PensumScoreHandler from "./pensum-score-handler";
import jsdom, {JSDOM} from "jsdom";
import fs from "fs";

fs.readFile("./docs/Volante Publicación de Medio Término.html", "utf-8", (err, data) => {
  if(err)
    return;

  console.log(data);

  const html = new JSDOM(data);
  const handler = new PensumScoreHandler(html);
  console.log(handler.notYetTaken);
  console.log(handler.notYetTakenForEachTrimester);
});

