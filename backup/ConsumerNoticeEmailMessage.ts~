class ConsumerNoticeEmailMessage {
  id: string;
  isRead: boolean;
  receivedDateTime: string;
  sender: { emailAddress: { name: string, address: string } } | undefined;
  subject: string;

  constructor(message: Partial<ConsumerNoticeEmailMessage>) {
    this.id =  message.id || "";
    this.isRead = message.isRead || false;
    this.receivedDateTime = message.receivedDateTime || "";
    this.sender = message.sender || undefined;
    this.subject = message.subject || "";
  }
}

export default ConsumerNoticeEmailMessage;
