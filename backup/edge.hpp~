#ifndef GRAPH_EDGE_HPP
#define GRAPH_EDGE_HPP

template <typename T>
class Node;

#include <iostream>
#include "node.hpp"

namespace Graph {
  template <typename T>
  class Edge {
    public:
      int id;
      int weight;
      Node<T>* node1;
      Node<T>* node2;

      Edge(Node<T>*, Node<T>*, int, int);
      ~Edge();
  };
}

#endif

