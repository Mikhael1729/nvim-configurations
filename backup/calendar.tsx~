import React, { FC } from "react";
import { MuiPickersUtilsProvider, KeyboardDatePicker, Calendar } from "@material-ui/pickers";
import DateFnsUtils from "@date-io/date-fns"
import "date-fns";

interface ICalendarProps {
  date: Date;
}

const CalendarProps: FC<ICalendarProps> = (props) => {
  const { date } = props;

  return (
    <MuiPickersUtilsProvider utils={DateFnsUtils}>
      <Calendar date={date} onChange={(date: any) => {}} />
    </MuiPickersUtilsProvider>
  )
}

export default CalendarProps;
