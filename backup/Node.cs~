namespace PrimAlgorithm
{
  public class Node<T>
  {
    public int Id { get; }
    public T Value { get; set; }
    public LinkedList<Edge<T>> Adjacents { get; } = new LinkedList<Edge<T>>();

    public Node(int id, T value) => (Id, Value) = (id, value);

    public override string ToString() => $"{Id}";
  }
}
